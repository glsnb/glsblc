<div class=\"page-home-hot-templates\">\n      <h1 class=\"page-home-hot-templates-header\">Hot Templates</h1>\n      <div class=\"page-home-hot-templates-cards\">\n        <Card\n          v-for=\"(item, index) in hotTemplates\"\n          :key=\"index\"\n          class=\"page-home-hot-templates-card\"\n        >\n          <p slot=\"title\">{{ item.title }}</p>\n          <p>\n            {{ item.description }}\n          </p>\n        </Card>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport service from \"@/service\";\nconst { getHomeData } = service.home;\nexport default {\n  data() {\n    return {\n      current: 0,\n      carouselData: [],\n      hotTemplates: []\n    };\n  },\n  mounted() {\n    getHomeData().then(res => {\n      const { carouselData, hotTemplates } = res.data;\n      this.carouselData = carouselData;\n      this.hotTemplates = hotTemplates;\n    });\n  }\n};\n</script>\n<style lang=\"less\">\n.page-home {\n  &-carousel-content {\n    border-radius: 4px;\n    height: 300px;\n    font-size: 24px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    background: #506b9e;\n    color: #fff;\n  }\n\n  &-hot-templates {\n    margin-top: 30px;\n    &-header {\n      text-align: center;\n    }\n\n    &-cards {\n      display: flex;\n      align-items: center;\n      justify-content: space-around;\n    }\n    &-card {\n      width: 30%;\n      display: inline-block;\n      margin: 20px;\n      height: 120px;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/page/home"}]}